ST	zz$$$$_17_gCapArmCtrl_Cmd_Enable
ST	zz$$$$_18_Eno_ComputeBlock
JMPCN ll$$$$_IF_5
Compute!	gCapArmCtrl.Cmd
ST	Internal.Cmd
ll$$$$_IF_5:
